{
	"name": "electronjs-aws-saml-extractor",
	"productName": "electronjs-aws-saml-extractor",
	"version": "0.0.0",
	"description": "SSO Provider for the AWS CLI",
	"license": "MIT",
	"repository": "https://github.com/akozich/electronjs-aws-saml-extractor.git",
	"author": {
		"name": "Aliaksei Kozich",
		"email": "alex.kozich@gmail.com",
		"url": "https://github.com/akozich/electronjs-aws-saml-extractor"
	},
	"main": "main.js",
    "bin": "cli.js",
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"lint-fix": "xo --fix",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --linux --windows",
		"release": "np"
	},
	"dependencies": {
		"electron-context-menu": "^0.15.1",
		"electron-debug": "^3.0.1",
		"electron-store": "^5.1.0",
		"electron-unhandled": "^3.0.1",
		"electron-updater": "^4.2.0",
		"electron-util": "^0.13.0",
		"aws-sdk": "^2.574.0",
		"electron-log": "^3.0.9",
		"electron-settings": "^3.2.0",
		"electron-shortcut-normalizer": "^1.0.0",
		"glob": "^7.1.6",
		"qs": "^6.9.1",
		"yargs": "^15.0.2"
	},
	"devDependencies": {
		"electron": "^7.1.1",
		"electron-builder": "^21.2.0",
		"np": "^5.1.3",
		"xo": "^0.25.3"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "com.akozich.electronjs-aws-saml-extractor",
		"mac": {
			"category": "public.app-category.social-networking",
			"darkModeSupport": true
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Network;Chat"
		}
	}
}
